{
  "expo": {
    "name": "lumitune-mobile",
    "slug": "lumitune",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/common/appLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "rgba(24, 43, 73, 0.3)"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.flowermaiden.lumitune"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "d08262e5-bc27-4b1f-9c67-5bee4b731738"
      }
    },
    "owner": "flowermaiden"
  }
}
